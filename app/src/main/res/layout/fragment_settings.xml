<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingsFragment" android:id="@+id/frameLayout2"
    android:background="#f8f8ff"
    android:orientation="horizontal">



    <Button
        android:text="Back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:id="@+id/buttonBack"
        app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp" android:layout_marginRight="16dp" android:layout_marginEnd="16dp"/>

    <CheckBox
        android:id="@+id/switch1"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:text="Receive Notifications" />




    </LinearLayout>